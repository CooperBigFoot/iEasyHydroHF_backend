# Cron job to run the ingestion command every 20 minutes
*/20 * * * * root . /etc/environment; /usr/local/bin/python /app/manage.py ingest >> /app/cron-ingestion.log 2>&1

# Cron job to run the lindas ingestion command every 22 minutes
*/22 * * * * root . /etc/environment; /usr/local/bin/python /app/manage.py query_lindas >> /app/cron-lindas.log 2>&1
